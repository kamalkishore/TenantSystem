@model IEnumerable<TenantSystem.Models.ElectricityMeterPerUnitPrices>

@{
    ViewBag.Title = "View Electricity Unit Price";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>View Electricity Unit Price</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditElectricityMeterUnitPrice", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
